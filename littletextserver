  ./build/main -p "32MiB" /workspace/dmeyer/data/dblp.xml
  ./build/main -p "32MiB" /workspace/dmeyer/data/dna
  ./build/main -p "32MiB" /workspace/dmeyer/data/pitches
  ./build/main -p "32MiB" /workspace/dmeyer/data/proteins
  ./build/main -p "32MiB" /workspace/dmeyer/data/sources
  ./build/main -p "32MiB" /workspace/dmeyer/data/english
    ./build/main -p "32MiB" /workspace/dmeyer/data/dblp.xml
    ./build/main -p "32MiB" /workspace/dmeyer/data/dna
